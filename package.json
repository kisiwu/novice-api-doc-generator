{
  "name": "@novice1/api-doc-generator",
  "private": false,
  "version": "0.0.1",
  "description": "Quickly generate API Documentation.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kisiwu/novice-api-doc-generator.git"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "doc": "typedoc",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' kaukau --require ts-node/register --file test --ext .test.ts",
    "testany": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\", \"noImplicitAny\": false}' kaukau --require ts-node/register --file test --ext .test.ts"
  },
  "keywords": [
    "openapi",
    "postman",
    "swagger"
  ],
  "author": "demingongo",
  "contributors": [
    "sbolingo"
  ],
  "license": "MIT",
  "devDependencies": {
    "@novice1/routing": "^1.0.10",
    "@types/chai": "^4.2.15",
    "@types/extend": "^3.0.1",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.21.0",
    "kaukau": "^0.2.2",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.29",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "extend": "^3.0.2",
    "joi": "^17.4.0",
    "tslib": "^2.1.0"
  }
}
